    "acorn": "^8.14.0",
    "acorn-walk": "^8.3.4",
    "chokidar": "^4.0.1",
    "crypto-js": "^4.2.0",
    "d3-dsv": "^3.0.1",
    "esbuild-wasm": "^0.24.0",
    "express": "^4.21.1",
    "get-port": "^7.1.0",
    "highlight.js": "^11.10.0",
    "idb-keyval": "^6.2.1",
