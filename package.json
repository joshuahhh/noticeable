{
  "name": "noticeable",
  "version": "0.0.5",
  "type": "module",
  "bin": {
    "notice": "./bin/notice.js"
  },
  "main": "lib/lib.js",
  "files": [
    "lib",
    "dist",
    "bin"
  ],
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "dev": "vite",
    "build": "vite build",
    "test:watch": "vitest watch",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "build-cli": "tsc --project tsconfig.node.json",
    "build-cli:watch": "tsc --watch --project tsconfig.node.json",
    "build-lib": "rm -rf lib && tsc --project tsconfig.lib.json && copyfiles -u 1 \"src/**/*.{js,css}\" lib",
    "prepublishOnly": "npm run build && npm run build-cli && npm run build-lib"
  },
